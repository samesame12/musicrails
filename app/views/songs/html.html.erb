<table>
  <thead>
    <tr>
    <th>Name</th>
    <th>Band</th>
    <th>Album</th>
    <th>Genre</th>
    <th>Chord</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.name %></td>
        <td><%= song.band %></td>
        <td><%= song.album %></td>
        <td><%= song.genre %></td>
        <td><%= song.chord %></td>
        </tr>
    <% end %>
  </tbody>
</table>